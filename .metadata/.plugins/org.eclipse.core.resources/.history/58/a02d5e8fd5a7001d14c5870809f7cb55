package azienda;

public class Guardia extends Employee implements Pagamento,InforzazioniBase{
	

	



	public Guardia(double fondo, String firstName, String lastName, int nMatricola) {
		super(fondo, firstName, lastName, nMatricola);
		// TODO Auto-generated constructor stub
	}


	@Override
	public double getSalary() {
		// TODO Auto-generated method stub
		return getPagaBase() + 500 ;
	}


}