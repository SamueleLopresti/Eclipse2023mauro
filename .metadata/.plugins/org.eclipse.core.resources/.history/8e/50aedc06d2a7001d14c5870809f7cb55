package azienda;

public class Account  implements Pagamento {
	
	

	public Account() {
		super();
	}
	double ammount;
	

	
	
	public void transferTo (Employee hiredEmployee,double ammount) {
		double temp = hiredEmployee.getAccount();
		temp += ammount;
		hiredEmployee.setAccount(temp);
		
		
	}
	@Override
	public double getSalary() {
		// TODO Auto-generated method stub
		return 0;
	}
	@Override
	public double getAccount() {
		// TODO Auto-generated method stub
		return ammount;
	}
	
	@Override
	public double setAccount(double account) {
		// TODO Auto-generated method stub
		return 0;
	}
	

}
