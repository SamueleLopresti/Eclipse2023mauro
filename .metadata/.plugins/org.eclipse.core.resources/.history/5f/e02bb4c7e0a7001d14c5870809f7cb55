package azienda;

public class Guardia extends Employee implements Pagamento,InforzazioniBase{
	

	



	

	public Guardia(String firstName, String lastName, int nMatricola, double fondo) {
		super(firstName, lastName, nMatricola, fondo);
		// TODO Auto-generated constructor stub
	}

	@Override
	public double getSalary() {
		// TODO Auto-generated method stub
		return getPagaBase() + 500 ;
	}
	
	public void setFondo(double fondo) {
		this.fondo = fondo;
	}
	


}