package azienda;

public class Programmatore extends Employee implements Pagamento,InforzazioniBase{
	

	

	



	public Programmatore(double fondo, String firstName, String lastName, int nMatricola) {
		super(fondo, firstName, lastName, nMatricola);
		// TODO Auto-generated constructor stub
	}
	@Override
	public double getSalary() {
		// TODO Auto-generated method stub
		return getPagaBase() +50 ;
	}
	
}
 