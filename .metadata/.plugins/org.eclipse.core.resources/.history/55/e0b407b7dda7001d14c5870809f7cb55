package azienda;

public class Account   {
	
	
    
	private double fondo;
	
	

	public Account(double fondo) {
	
		this.fondo = fondo;
	}

	
	

	
	
	public void transferTo (Employee employee, double ammount) {
		employee.setAccount(employee.getAccount().fondo+ammount);
		this.fondo -= ammount;
		
	}
	
	
	
	
	
	
	
	

	
	
	

}
